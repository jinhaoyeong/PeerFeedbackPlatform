{
  "name": "peer-feedback-platform",
  "version": "1.0.0",
  "description": "A safe and anonymous platform for giving and receiving constructive feedback",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "dev:socket": "node socket-only.js",
    "dev:turbo": "next dev --turbo --port 3002",
    "dev:debug": "NODE_OPTIONS=--inspect next dev --port 3002",
    "dev:standard": "next dev --port 3002",
    "dev:legacy": "next dev",
    "build": "node scripts/update-schema.js && prisma generate && node scripts/db-push-if-prod.js && next build --webpack",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:inspect": "node scripts/inspect-db.js",
    "db:delete": "node scripts/delete-data-safely.js",
    "db:seed": "node scripts/create-test-users.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "feedback",
    "anonymous",
    "peer-review",
    "constructive",
    "education"
  ],
  "author": "Peer Feedback Platform",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "next": "^16.0.7",
    "nodemailer": "^7.0.11",
    "otplib": "^12.0.1",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "recharts": "^3.5.0",
    "sentiment": "^5.0.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "util": "^0.12.5",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/nodemailer": "^7.0.4",
    "@types/qrcode": "^1.5.6",
    "@types/sentiment": "^5.0.4"
  }
}
